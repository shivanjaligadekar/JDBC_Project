<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean class="example.spring.core.BeanImpl" id="myBean"></bean>
	
	<bean class="example.spring.core.BeanImpl" id="myBean2" lazy-init="true"></bean>
	
	<bean class="example.spring.core.BeanImpl" id="myBean3" lazy-init="true"></bean>
	
	<bean class="example.spring.core.BeanImpl" id="myBean4" lazy-init="true" scope="prototype"></bean>

</beans>